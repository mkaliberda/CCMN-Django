{
  "env": "default",
  "host": "localhost",
  "port": "8080",
  "jsSourcePath": "src/js",
  "s3": {
    "s3Deploy": false,
    "bucket": "if-any",
    "defaultCDNBase": "if-any"
  },
  "optimization": {
    "commonMinCount": 3,
    "cssExclusion": true,
    "analyzeMode": false,
    "analyze": {
      "port": 8888
    }
  },
  "browserSync": {
    "active": true,
    "port": 3002
  },
  "html": [
    {
			"name": "homepage",
      "title": "Everything is Awesome",
      "template": "_default.html",
      "filename": "index.html"
    }
  ],
  "app": {
    "description": "config inside 'app' will be injected to your frontend code automatically"
  }
}
